{
  "name": "pilotidupc",
  "displayName": "PilotIDUPC",
  "description": "Automatic Python Fibonacci generator using GitHub Copilot",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "pilotidupc.createFibonacci",
        "title": "Create Fibonacci Function"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.1",
    "@types/node": "^14.x.x",
    "@types/vscode": "^1.75.0",
    "typescript": "^4.x.x"
  },
  "publisher": "tu-publisher-id",
  "dependencies": {
    "dotenv": "^16.4.7"
  }
}
